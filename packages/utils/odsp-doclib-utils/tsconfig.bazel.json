{
	"compilerOptions": {
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"exactOptionalPropertyTypes": false,
		"forceConsistentCasingInFileNames": true,
		"lib": ["ES2022", "DOM"],
		"module": "Node16",
		"moduleResolution": "Node16",
		"noUncheckedIndexedAccess": false,
		"outDir": "./lib",
		"resolveJsonModule": true,
		"rootDir": "./src",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": false,
		"target": "ES2022",
		"types": [],
		"paths": {
			"@fluid-internal/client-utils": ["../../common/client-utils/lib/index.d.ts"],
			"@fluid-internal/client-utils/*": ["../../common/client-utils/lib/*"],
			"@fluidframework/core-interfaces": ["../../common/core-interfaces/lib/index.d.ts"],
			"@fluidframework/core-interfaces/*": ["../../common/core-interfaces/lib/*"],
			"@fluidframework/core-utils": ["../../common/core-utils/lib/index.d.ts"],
			"@fluidframework/core-utils/internal": ["../../common/core-utils/lib/index.d.ts"],
			"@fluidframework/core-utils/*": ["../../common/core-utils/lib/*"],
			"@fluidframework/driver-definitions": ["../../common/driver-definitions/lib/index.d.ts"],
			"@fluidframework/driver-definitions/internal": ["../../common/driver-definitions/lib/index.d.ts"],
			"@fluidframework/driver-definitions/*": ["../../common/driver-definitions/lib/*"],
			"@fluidframework/driver-utils": ["../../../packages/loader/driver-utils/lib/index.d.ts"],
			"@fluidframework/driver-utils/internal": ["../../../packages/loader/driver-utils/lib/index.d.ts"],
			"@fluidframework/driver-utils/*": ["../../../packages/loader/driver-utils/lib/*"],
			"@fluidframework/odsp-driver-definitions": ["../../../packages/drivers/odsp-driver-definitions/lib/index.d.ts"],
			"@fluidframework/odsp-driver-definitions/internal": ["../../../packages/drivers/odsp-driver-definitions/lib/index.d.ts"],
			"@fluidframework/odsp-driver-definitions/*": ["../../../packages/drivers/odsp-driver-definitions/lib/*"],
			"@fluidframework/telemetry-utils": ["../telemetry-utils/lib/index.d.ts"],
			"@fluidframework/telemetry-utils/internal": ["../telemetry-utils/lib/index.d.ts"],
			"@fluidframework/telemetry-utils/*": ["../telemetry-utils/lib/*"]
		}
	},
	"include": ["src/**/*"],
	"exclude": ["src/test/**/*"]
}
